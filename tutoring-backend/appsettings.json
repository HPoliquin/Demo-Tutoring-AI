{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SendGrid": {
    "ApiKey": "YOUR_SENDGRID_API_KEY"
  },
  "ConnectionStrings": {
    "PostgresConnection": "Host={DB_HOST};Port={DB_PORT};Database={DB_NAME};Username={DB_USER};Password={DB_PASSWORD}"
  },
  "DotNetEnv": {
    "FilePath": "../.env",
    "Enabled": true
  }
}
